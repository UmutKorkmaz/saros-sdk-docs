{
  "name": "saros-sdk-documentation",
  "version": "1.0.0",
  "description": "Comprehensive developer documentation for Saros Finance SDKs",
  "homepage": "https://saros-finance.github.io/saros-sdk-docs",
  "private": true,
  "workspaces": [
    "code-examples/typescript/*"
  ],
  "scripts": {
    "serve": "node server.js",
    "dev": "npm run serve",
    "docs": "npm run serve",
    "serve:static": "npx http-server public -p 8080 -c-1 --cors",
    "build": "npm run build:clean && npm run build:static",
    "build:clean": "rm -rf public/docs/ public/code-examples/ || true",
    "build:static": "npm run copy:docs && npm run copy:examples",
    "copy:docs": "echo 'Static build handled by GitHub Actions'",
    "copy:examples": "echo 'Static build handled by GitHub Actions'",
    "build:examples": "npm run build --workspaces",
    "predeploy": "npm run build:static",
    "deploy": "gh-pages -d public",
    "test": "npm run test --workspaces",
    "dev:swap": "npm run dev -w code-examples/typescript/01-swap-with-slippage",
    "dev:compound": "npm run dev -w code-examples/typescript/02-auto-compound",
    "dev:il": "npm run dev -w code-examples/typescript/03-impermanent-loss-calc",
    "dev:range": "npm run dev -w code-examples/typescript/04-dlmm-range-orders",
    "dev:routing": "npm run dev -w code-examples/typescript/05-multi-hop-routing",
    "install:all": "npm install --workspaces",
    "clean": "npm run clean --workspaces",
    "lint": "npm run lint --workspaces",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/saros-finance/sdk-documentation"
  },
  "keywords": [
    "saros",
    "solana",
    "defi",
    "amm",
    "dlmm",
    "sdk",
    "documentation",
    "typescript",
    "rust"
  ],
  "author": "Saros Finance",
  "license": "MIT",
  "devDependencies": {
    "gh-pages": "^6.1.0",
    "http-server": "^14.1.1",
    "prettier": "^3.1.0",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}