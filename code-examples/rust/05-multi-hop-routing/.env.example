# Solana Network Configuration
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com
SOLANA_WS_URL=wss://api.mainnet-beta.solana.com

# Alternative endpoints for development/testing
# SOLANA_RPC_URL=https://api.devnet.solana.com
# SOLANA_RPC_URL=http://localhost:8899

# Wallet Configuration
PRIVATE_KEY=your_base58_encoded_private_key_here
# Example: 5GrwvaEF5zXb26Fz9rcQpDWS57CtERHpNehXCPcNoHRxv9FVmpu4WndMi6WvV4XprMHmBWz8jJuBHvDJhJCFXbVkbzYx

# Trading Parameters
MAX_SLIPPAGE=0.01                    # 1% maximum slippage tolerance
DEFAULT_DEADLINE_MINUTES=5           # Default transaction deadline
MAX_ROUTE_HOPS=5                     # Maximum hops in a route
MIN_LIQUIDITY_USD=1000              # Minimum pool liquidity to consider

# Gas and Fee Configuration
GAS_PRICE_MULTIPLIER=1.2            # Multiply base gas price by this factor
MAX_PRIORITY_FEE_SOL=0.001          # Maximum priority fee in SOL
BASE_GAS_LIMIT=200000               # Base gas limit for transactions
PER_HOP_GAS=100000                  # Additional gas per routing hop

# MEV Protection
ENABLE_MEV_PROTECTION=false         # Enable MEV protection strategies
PRIVATE_MEMPOOL_URL=                # Private mempool endpoint (if available)
SLIPPAGE_BUFFER=0.005               # Additional slippage buffer for MEV protection
USE_FLASHLOAN_ARBITRAGE=false       # Enable flash loan arbitrage execution

# Cache Configuration
ROUTE_CACHE_TTL_SECONDS=30          # Route cache time-to-live
POOL_DATA_REFRESH_SECONDS=60        # Pool data refresh interval
PRICE_CACHE_TTL_SECONDS=10          # Price data cache TTL
MAX_CACHE_SIZE=1000                 # Maximum cached routes

# Graph Algorithm Parameters
GRAPH_REBUILD_INTERVAL_MINUTES=30   # How often to rebuild the pool graph
MAX_GRAPH_NODES=5000               # Maximum tokens in graph
MAX_GRAPH_EDGES=50000              # Maximum pools in graph
CONNECTIVITY_ANALYSIS_DEPTH=3       # Max depth for connectivity analysis

# Arbitrage Detection
MIN_ARBITRAGE_PROFIT_USD=10         # Minimum profit to consider opportunity
MAX_ARBITRAGE_CYCLE_LENGTH=4        # Maximum cycle length for arbitrage
ARBITRAGE_SCAN_INTERVAL_SECONDS=30  # How often to scan for opportunities
ENABLE_CROSS_DEX_ARBITRAGE=false    # Enable arbitrage across different DEXs

# Route Optimization Weights
WEIGHT_PRICE_IMPACT=0.4             # Price impact importance (0.0-1.0)
WEIGHT_GAS_COST=0.2                 # Gas cost importance (0.0-1.0)
WEIGHT_LIQUIDITY_DEPTH=0.3          # Liquidity depth importance (0.0-1.0)
WEIGHT_EXECUTION_CERTAINTY=0.1      # Execution certainty importance (0.0-1.0)

# Split Route Configuration
MAX_SPLIT_ROUTES=4                  # Maximum number of split routes
MIN_SPLIT_AMOUNT_USD=100            # Minimum amount to consider splitting
SPLIT_OPTIMIZATION_THRESHOLD=10000  # USD threshold for split optimization

# Execution Settings
MAX_RETRIES=3                       # Maximum transaction retry attempts
RETRY_DELAY_MS=1000                 # Delay between retries in milliseconds
CONFIRMATION_TIMEOUT_SECONDS=30     # Transaction confirmation timeout
ENABLE_PARALLEL_EXECUTION=false     # Enable parallel route execution
MAX_PARALLEL_ROUTES=3               # Maximum concurrent route executions

# Monitoring and Metrics
ENABLE_METRICS=true                 # Enable performance metrics collection
METRICS_PORT=9090                   # Prometheus metrics port
LOG_LEVEL=info                      # Logging level (trace, debug, info, warn, error)
LOG_FORMAT=json                     # Log format (json, pretty)

# Database Configuration (optional)
DATABASE_URL=                       # Database URL for persistent storage
ENABLE_ROUTE_HISTORY=false          # Store route execution history
ENABLE_PERFORMANCE_ANALYTICS=false  # Store detailed performance metrics

# External APIs
COINGECKO_API_KEY=                  # CoinGecko API key for price data
JUPITER_API_URL=https://quote-api.jup.ag/v6  # Jupiter aggregator API
ENABLE_EXTERNAL_PRICE_FEEDS=false   # Use external price feeds

# Development and Testing
ENABLE_SIMULATION_MODE=false        # Run in simulation mode (no real transactions)
MOCK_RPC_RESPONSES=false           # Use mock RPC responses for testing
ENABLE_DETAILED_LOGGING=false      # Enable detailed debug logging
TEST_TOKEN_ADDRESSES=              # Comma-separated test token addresses

# Performance Tuning
WORKER_THREADS=4                    # Number of async worker threads
ROUTE_FINDER_TIMEOUT_MS=5000       # Route finding timeout
ARBITRAGE_DETECTION_TIMEOUT_MS=10000 # Arbitrage detection timeout
GRAPH_ALGORITHM_TIMEOUT_MS=30000    # Graph algorithm timeout

# Security Settings
ENABLE_SIGNATURE_VERIFICATION=true  # Verify transaction signatures
MAX_TRANSACTION_SIZE_BYTES=1232     # Maximum transaction size
ENABLE_RATE_LIMITING=true          # Enable API rate limiting
REQUESTS_PER_MINUTE=60             # Rate limit for API requests

# Notification Settings (optional)
ENABLE_NOTIFICATIONS=false          # Enable notifications
DISCORD_WEBHOOK_URL=               # Discord webhook for notifications
TELEGRAM_BOT_TOKEN=                # Telegram bot token
TELEGRAM_CHAT_ID=                  # Telegram chat ID
SLACK_WEBHOOK_URL=                 # Slack webhook URL

# Advanced Configuration
ENABLE_PROFILING=false             # Enable performance profiling
PROFILE_OUTPUT_DIR=./profiles      # Directory for profiling output
ENABLE_MEMORY_ANALYSIS=false       # Enable memory usage analysis
OPTIMIZE_FOR_LATENCY=true          # Optimize for low latency vs throughput

# Pool Filtering
MIN_POOL_VOLUME_24H=1000           # Minimum 24h volume to consider pool
MAX_POOL_AGE_DAYS=30               # Maximum pool age to consider
BLACKLISTED_POOLS=                 # Comma-separated blacklisted pool addresses
WHITELISTED_TOKENS=                # Comma-separated whitelisted token addresses

# Risk Management
MAX_EXPOSURE_PER_TOKEN_USD=10000   # Maximum exposure per token
MAX_TOTAL_EXPOSURE_USD=100000      # Maximum total exposure
ENABLE_STOP_LOSS=false             # Enable automatic stop-loss
STOP_LOSS_THRESHOLD=0.05           # Stop-loss threshold (5%)

# Backup and Recovery
ENABLE_STATE_BACKUP=false          # Enable state backup
BACKUP_INTERVAL_HOURS=24           # Backup interval in hours
BACKUP_RETENTION_DAYS=7            # How long to retain backups
BACKUP_STORAGE_PATH=./backups      # Backup storage directory