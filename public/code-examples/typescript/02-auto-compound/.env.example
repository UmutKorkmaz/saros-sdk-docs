# Solana Network Configuration
SOLANA_NETWORK=devnet
# Options: devnet, mainnet-beta, testnet

SOLANA_RPC_URL=https://api.devnet.solana.com
# For better performance, use dedicated RPC:
# HELIUS_RPC_URL=https://rpc.helius.xyz/?api-key=YOUR_API_KEY
# QUICKNODE_RPC_URL=https://YOUR_QUICKNODE_ENDPOINT

# Wallet Configuration
WALLET_PRIVATE_KEY=YOUR_BASE58_PRIVATE_KEY_HERE
# Generate: solana-keygen new
# Export: solana-keygen show --output json

# Auto-Compound Configuration
AUTO_COMPOUND_ENABLED=true
# Enable/disable auto-compound functionality

COMPOUND_INTERVAL=3600000
# Compound interval in milliseconds (3600000 = 1 hour)
# Common values:
# 3600000 = 1 hour
# 7200000 = 2 hours
# 14400000 = 4 hours
# 28800000 = 8 hours
# 86400000 = 24 hours

MIN_REWARD_THRESHOLD=1.0
# Minimum rewards in USD before compounding

REINVEST_PERCENTAGE=100
# Percentage of rewards to reinvest (0-100)

# Strategy Configuration
STRATEGY_TYPE=LP
# Options: LP, STAKING, FARMING

LP_POOL_ADDRESS=POOL_ADDRESS_HERE
# LP pool address for liquidity provision

STAKING_POOL_ADDRESS=STAKING_ADDRESS_HERE
# Staking pool address

FARM_ADDRESS=FARM_ADDRESS_HERE
# Farm address for yield farming

# Multi-Strategy Weights (must sum to 1.0)
LP_WEIGHT=0.5
STAKING_WEIGHT=0.3
FARMING_WEIGHT=0.2

# Gas Configuration
MAX_GAS_PRICE=0.01
# Maximum gas price in SOL

GAS_OPTIMIZATION_ENABLED=true
# Enable gas optimization features

PRIORITY_FEE_LEVEL=medium
# Options: low, medium, high, urgent

# Risk Management
MAX_SLIPPAGE=1.0
# Maximum slippage tolerance in percentage

EMERGENCY_WITHDRAW_ENABLED=false
# Enable emergency withdraw functionality

REBALANCE_THRESHOLD=10
# Rebalance when allocation deviates by this percentage

MAX_POSITION_SIZE=10000
# Maximum position size in USD

# Performance Tracking
TRACK_PERFORMANCE=true
# Enable performance tracking

PERFORMANCE_LOG_INTERVAL=3600000
# Log performance every X milliseconds

CALCULATE_TAX_IMPACT=false
# Calculate after-tax returns

TAX_RATE=30
# Tax rate percentage for calculations

# Notification Configuration
ENABLE_NOTIFICATIONS=false
# Enable notification system

# Webhook Configuration
WEBHOOK_URL=
# Generic webhook URL for notifications

# Discord Configuration
DISCORD_WEBHOOK=
# Discord webhook URL
# Create: Server Settings > Integrations > Webhooks

# Telegram Configuration
TELEGRAM_BOT_TOKEN=
# Telegram bot token from @BotFather

TELEGRAM_CHAT_ID=
# Telegram chat ID (can be user or group ID)

# Slack Configuration
SLACK_WEBHOOK=
# Slack incoming webhook URL

# Email Configuration (requires additional setup)
EMAIL_NOTIFICATIONS=false
EMAIL_ADDRESS=
SMTP_HOST=
SMTP_PORT=587
SMTP_USER=
SMTP_PASS=

# Alert Thresholds
ALERT_ON_FAILURE=true
# Send alert on compound failure

ALERT_ON_HIGH_GAS=true
# Alert when gas exceeds threshold

HIGH_GAS_THRESHOLD=0.005
# Gas threshold in SOL

ALERT_ON_LOW_BALANCE=true
# Alert on low wallet balance

LOW_BALANCE_THRESHOLD=0.1
# Balance threshold in SOL

# Scheduling Configuration
USE_CRON_SCHEDULE=false
# Use cron expression instead of interval

CRON_EXPRESSION=0 */4 * * *
# Cron expression (every 4 hours)
# Examples:
# 0 * * * * = Every hour
# 0 */2 * * * = Every 2 hours
# 0 0 * * * = Daily at midnight
# 0 0 * * 1 = Weekly on Monday

# Advanced Configuration
SIMULATE_BEFORE_COMPOUND=true
# Simulate transactions before execution

USE_FLASHLOAN_FOR_COMPOUND=false
# Use flashloans to compound without gas

COMPOUND_DURING_LOW_CONGESTION=true
# Only compound during low network congestion

LOW_CONGESTION_TPS=1500
# TPS threshold for low congestion

# Pool-Specific Configuration
POOL_1_ADDRESS=
POOL_1_STRATEGY=LP
POOL_1_INTERVAL=3600000
POOL_1_MIN_THRESHOLD=1.0

POOL_2_ADDRESS=
POOL_2_STRATEGY=STAKING
POOL_2_INTERVAL=7200000
POOL_2_MIN_THRESHOLD=0.5

POOL_3_ADDRESS=
POOL_3_STRATEGY=FARMING
POOL_3_INTERVAL=86400000
POOL_3_MIN_THRESHOLD=2.0

# Analytics Configuration
ENABLE_ANALYTICS=false
# Send anonymous usage statistics

ANALYTICS_ENDPOINT=
# Analytics server endpoint

# Logging Configuration
LOG_LEVEL=info
# Options: error, warn, info, debug, trace

LOG_TO_FILE=true
# Save logs to file

LOG_MAX_SIZE=10485760
# Max log file size in bytes (10MB)

LOG_MAX_FILES=10
# Maximum number of log files to keep

LOG_PERFORMANCE_METRICS=true
# Log detailed performance metrics

# Development/Testing
NODE_ENV=development
# Options: development, production, test

DRY_RUN_MODE=false
# Run in simulation mode without executing transactions

USE_MOCK_DATA=false
# Use mock data for testing

MOCK_REWARDS_RANGE=1,10
# Mock rewards range (min,max)

# Security
ENABLE_2FA=false
# Require 2FA for critical operations

ENCRYPTION_KEY=
# Key for encrypting sensitive data

MAX_DAILY_COMPOUNDS=100
# Maximum compounds per day (rate limiting)

WHITELIST_POOLS=
# Comma-separated list of allowed pool addresses

BLACKLIST_TOKENS=
# Comma-separated list of blocked token addresses